<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DeliveryApp.Views.LogInPage">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="AliceBlue">
                <Label Text="Welcome log in to Xamarin.Forms!" />
            </Frame>
            <Frame BackgroundColor="AntiqueWhite">
                <Entry BackgroundColor="AntiqueWhite" Placeholder="Enter your email" Text="{Binding Email}" ></Entry>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>